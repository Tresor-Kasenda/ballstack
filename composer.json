{
    "name": "tresorkasenda/ballstack",
    "description": "A simple stack implementation in Laravel, Livewire and Alpine.js",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "livewire",
        "blade",
        "ui",
        "components",
        "livewire-packages",
        "livewire-components",
        "alpinejs",
        "boostrap 5"
    ],
    "authors": [
        {
            "role": "Software Developer",
            "name": "Tresor-Kasenda",
            "email": "tresorkasendat@gmail.com",
            "homepage": "https://tresorkasenda.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tresorkasenda\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tresorkasenda\\Tests\\": "tests"
        }
    },
    "require": {
        "illuminate/support": "^10.47",
        "livewire/livewire": "^3.4.9"
    },
    "require-dev": {
        "larastan/larastan": "^2.8",
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.21",
        "pestphp/pest": "^2.33"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tresorkasenda\\BallStackServiceProvider"
            ]
        }
    },
    "scripts": {
        "analyze": [
            "./vendor/bin/pint",
            "./vendor/bin/pest"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
